
{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "requireConfigFile": false,
        "babelOptions": {
            "babelrc": false,
            "configFile": false
        }
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "no-console": "off"
    }
}
